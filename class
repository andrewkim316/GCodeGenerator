class gCodeGenerator():
  x
  y
  z
  duration
  timestep
  t = symbols('t')
  
  def __init__(fx,fy,fz,d,ts):
    x = fx
    y = fy
    z = fz
    duration = d
    timestep = ts
    
  def changeTimestep(ts):
    t = ts
  
  def offset(off):
    x += off
    y += off
    z += off
  
  def setZero:
    return "G92"
