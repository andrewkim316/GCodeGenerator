class gCodeGenerator():
  
  def __init__(self,fx,fy,fz,dur,ts):
    self.x = fx
    self.y = fy
    self.z = fz
    self.d = dur
    self.t = ts
    
  def changeTimestep(self,ts):
    self.t = ts
  
  def offset(off):
    self.x += off
    self.y += off
    self.z += off
  
  def scale(s):
    self.x *= s
    self.y *= s
    self.z *= s
    
  def setZero:
    return "G92"
